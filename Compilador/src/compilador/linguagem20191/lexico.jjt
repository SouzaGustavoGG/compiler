    | <REAL> {AcoesSemanticas.palavraReservadaFloat_7(ec);}
    | <CHAR> {AcoesSemanticas.palavraReservadaString_8(ec);}
    | <BOOLEAN> {AcoesSemanticas.palavraReservadaLogic_9(ec);}
}

void ListaIdentConst() throws ErroSemantico: {} {
    <IDENTIFIER>{AcoesSemanticas.identificadorConstante_10(ec, ts, t.image);}ListaIdentConst1()
}

void ListaIdentConst1() throws ErroSemantico: {} {
    <COMMA>ListaIdentConst() | {}
}

void ListaIdentVar() throws ErroSemantico: {} {
    <IDENTIFIER>{AcoesSemanticas.identificadorVariavel_11(ec, ts, t.image);}Indice(){AcoesSemanticas.identificadorVariavelETamanhoIndexado_12(ec, ts, codigo);} ListaIdentVar1()
}

void ListaIdentVar1() throws ErroSemantico: {} {
    <COMMA>ListaIdentVar() | {}
