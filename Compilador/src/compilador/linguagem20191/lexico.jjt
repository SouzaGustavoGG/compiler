}

void Valor() throws ErroSemantico: {} {
    <LITERAL_CONST_DOUBLE_QUOTES> | <LITERAL_CONST_SINGLE_QUOTES> | <NATURAL_NUMBER> | <REAL_NUMBER>
}

void CorpoPrograma() throws ErroSemantico: {} {
    <EXECUTE><OPEN_KEY>ListaDeComandos()<CLOSE_KEY>
}

void ListaDeComandos() throws ErroSemantico: {} {
    Comando()<DOT>ListaDeComandos2()
}

void ListaDeComandos2() throws ErroSemantico: {} {
