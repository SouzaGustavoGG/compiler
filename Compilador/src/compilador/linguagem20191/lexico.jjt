}

void Comando() throws ErroSemantico: {} {
	Atribuicao() | Entrada() | Saida() | Selecao() | Repeticao()
}

void Atribuicao () throws ErroSemantico: {} {
	<SET>{AcoesSemanticas.comandoAtribuicao_14(ec);}Expressao()<TO>ListaIdentVar(){AcoesSemanticas.fimComandoAtribuicao_15(ec,codigo);}
}

void Entrada() throws ErroSemantico: {} {
	<GET> {AcoesSemanticas.comandoEntradaDados_16(ec);} <OPEN_KEY> ListaIdentVar() <CLOSE_KEY>
}	
	
void Saida() throws ErroSemantico: {} {
	<PUT><OPEN_KEY> ListaIdentEOuConst() <CLOSE_KEY>
}

void ListaIdentEOuConst() throws ErroSemantico: {} {
	Item() {AcoesSemanticas.mensagemEmComandoSaida_17(ec,codigo);} ListaIdentEOuConst2()
}

void ListaIdentEOuConst2() throws ErroSemantico: {} {
	<COMMA> ListaIdentEOuConst() | {}
}

void Item() throws ErroSemantico: {} {
	<IDENTIFIER> {AcoesSemanticas.identificadorComandoSaida_18(ec, ts, codigo, t.image);}Indice(){AcoesSemanticas.variavelArrayComandoDeSaida_19(ec, ts, codigo);}
	| <NATURAL_NUMBER>{AcoesSemanticas.constanteInteiraComandoSaida_20(ec,codigo,valor.image);}
	| <REAL_NUMBER>{AcoesSemanticas.constanteRealComandoSaida_21(ec,codigo,valor.image);}
	| (<LITERAL_CONST_DOUBLE_QUOTES> | <LITERAL_CONST_SINGLE_QUOTES>){AcoesSemanticas.constanteLiteralComandoSaida_22(ec,codigo,valor.image);}
}

void Selecao() throws ErroSemantico: {} {
 	<VERIFY> Expressao() <IS> Clausulas() {AcoesSemanticas.fimComandoSelecao_23(ec,codigo);}
}

void Clausulas() throws ErroSemantico: {} {
	<TRUE> {AcoesSemanticas.palavraReservadaTrue_24(ec,codigo,ts);} <OPEN_KEY> ListaComandos() <CLOSE_KEY> Falsa()
	|<FALSE> {AcoesSemanticas.palavraReservadaUntrue_25(ec,codigo,ts);} <OPEN_KEY> ListaComandos() <CLOSE_KEY> Verdadeira()
