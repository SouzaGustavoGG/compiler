	|<WHILE>{AcoesSemanticas.inicioComandoRepeticao_29(ec);} Expressao() {AcoesSemanticas.comandoRepeticao_30(ec,codigo);} <IS> <TRUE> <DO> <OPEN_KEY> ListaComandos() <CLOSE_KEY> {AcoesSemanticas.fimComandoRepeticao_31(ec,codigo);}
}

void Expressao() throws ErroSemantico: {} {
	ExpressaoAritmeticaOuLogica() Expressao1()
}

void Expressao1() throws ErroSemantico: {} { 
	<EQUALS> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalIgual_32(ec, codigo);}
	|<DIFFERENT> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalDiferente_33(ec, codigo);}
	|<LESS> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalMenor_34(ec, codigo);}
	|<GREATER> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalMaior_35(ec, codigo);}
	|<LESS_EQUAL> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalMenorIgual_36(ec, codigo);}
	|<GREATER_EQUAL> ExpressaoAritmeticaOuLogica(){AcoesSemanticas.operacaoRelacionalMaiorIgual_37(ec, codigo);}
	| {}
}

void ExpressaoAritmeticaOuLogica() throws ErroSemantico: {} {
	Termo2() MenorPrioridade()
}

void MenorPrioridade() throws ErroSemantico: {} {
	<PLUS> Termo2() MenorPrioridade() {AcoesSemanticas.operacaoAritmeticaAdicao_38(ec,codigo);}
	|<MINUS> Termo2() MenorPrioridade() {AcoesSemanticas.operacaoAritmeticaSubtracao_39(ec,codigo);}
	|<PIPE> Termo2() MenorPrioridade() {AcoesSemanticas.operacaoLogicaOU_40(ec,codigo);}
	| {}
}

void Termo2() throws ErroSemantico: {} {
	 Termo1() MediaPrioridade()
}
	 
void MediaPrioridade() throws ErroSemantico: {} {
	<MULTIPLY> Termo1() MediaPrioridade() {AcoesSemanticas.operacaoAritmeticaMultiplicao_41(ec,codigo);}
	|<DIVIDER> Termo1() MediaPrioridade() {AcoesSemanticas.operacaoAritmeticaDivReal_42(ec,codigo);}
	|<PERCENT> Termo1() MediaPrioridade() {AcoesSemanticas.operacaoAritmeticaDivInteira_43(ec,codigo);}
	|<MOD> Termo1() MediaPrioridade() {AcoesSemanticas.operacaoAritmeticaRestoDivInteira_44(ec,codigo);}
	|<AMPERSAND> Termo1() MediaPrioridade() {AcoesSemanticas.operacaoLogicaE_45(ec, codigo);}
	| {}
}

void Termo1() throws ErroSemantico: {} { 
	Elemento() MaiorPrioridade()
}

void MaiorPrioridade() throws ErroSemantico: {} {
	<POW> Elemento() MaiorPrioridade() {AcoesSemanticas.operacaoAritmeticaPotencia_46(ec,codigo);} | {}
}
	
void Elemento() throws ErroSemantico: {} {
	 <IDENTIFIER> {AcoesSemanticas.aidentificadorComandoSaida_18(ec, ts, codigo, t.image);} Indice() {AcoesSemanticas.variavelArrayComandoDeSaida_19(ec, ts, codigo);}
	| <NATURAL_NUMBER> {AcoesSemanticas.constanteInteiraComandoSaida_20(ec, codigo, valor.image);}
	| <REAL_NUMBER> {AcoesSemanticas.constanteRealComandoSaida_21(ec, codigo, valor.image);}
	| (<LITERAL_CONST_DOUBLE_QUOTES> | <LITERAL_CONST_SINGLE_QUOTES>) {AcoesSemanticas.constanteLiteralComandoSaida_22(ec,codigo, valor.image);}
	| <TRUE> {AcoesSemanticas.constanteLogicaTrue_47(ec,codigo);}
	| <FALSE> {AcoesSemanticas.constanteLogicaUntrue_48(ec,codigo);}
	| <OPEN_PARENTHESIS> Expressao() <CLOSE_PARENTHESIS>
	| <EXCLAMATION> <OPEN_PARENTHESIS> Expressao() <CLOSE_PARENTHESIS> {AcoesSemanticas.operacaoLogicaNao_49(ec,codigo);}
